<?xml version="1.0"?>
<package>
  <metadata>
    <id>jquery.sparkline.mvc4</id>
    <title>jQuery sparklines for ASP.Net MVC4</title>
    <version>0.1.0</version>
    <authors>Gareth Watts, Marijn van der Zee</authors>
    <description>This installs the jquery.sparkline library, with script bundles and some helpers.</description>
    <projectUrl>https://github.com/serra</projectUrl>
    <releaseNotes>https://github.com/serra</releaseNotes>
    <dependencies>
      	<dependency id="jquery" version="[1.4.3,1.9)" />
        <dependency id="microsoft.aspnet.web.optimization" version="1.0.0"/>
    </dependencies> 
  </metadata>
  <files>
    <file src="Sparkline\Scripts\*.*" target="Content\Scripts"></file>
    <file src="Sparkline\App_Start\*.*" target="Content\App_Start"></file>
    <file src="Sparkline\Tools\*.*" target="tools"></file>
  </files>
</package>